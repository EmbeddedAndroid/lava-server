{% set baud_rate = baud_rate|default(115200) %}
{% set device_type = "imx7d-sbc-iot-imx7" %}
{% set bootloader_prompt = bootloader_prompt|default('CL-SOM-iMX7') %}
{% set bootm_kernel_addr = '0x80800000' %}
{% set bootm_ramdisk_addr = '0x82000000' %}
{% set bootm_dtb_addr = '0x83000000' %}
{% set bootz_kernel_addr = '0x80800000' %}
{% set bootz_ramdisk_addr = '0x82000000' %}
{% set bootz_dtb_addr = '0x83000000' %}
{% set usb_uuid = usb_uuid|default('') %}
{% set usb_device_id = usb_device_id|default('') %}
{% set uboot_mkimage_arch = 'arm' %}